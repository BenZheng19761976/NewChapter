{
    "compilerOptions": {
        "outDir": "./portal/",
        "sourceMap": true,
        "noImplicitAny": false,
        "target": "es5",
        "jsx": "react",
        "lib": [
            "es5",
            "es6",
            "dom"
        ],
        "esModuleInterop": true
    },
    "include": [
        "./src/**/*"
    ],
    "awesomeTypescriptLoaderOptions": {
        "reportFiles": [
            "./src/**/*"
        ]
    }
}
